printline("While loop:")
while loop_index < 2 repeat
    while loop_index < 2 repeat
         printline("Outer {{ outer loop_index }}")
         printline("Inner {{ loop_index }}")
    endwhile
endwhile

printline("Until Loop")
until loop_index > 1 repeat
    until loop_index > 1 repeat
         printline("Outer {{ outer loop_index }}")
         printline("Inner {{ loop_index }}")
    enduntil
enduntil

