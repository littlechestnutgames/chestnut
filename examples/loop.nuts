printline("Iterate loop:")
let a = ["Zero", "One"]
iterate a with item
    iterate a with item2
        printline("Items: {{ item }} {{ item2 }}")
        printline("Outer: {{ outer loop_index }}")
	printline("Inner: {{ loop_index }}")
    enditerate
