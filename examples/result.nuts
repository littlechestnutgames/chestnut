fn main(variadic args : String)
    let res = 0
    fn add_one(val : Integer) returns Result
        return Result.new(val + 1, null)
    endfn

    let initial_result = Result.new(5, null)

    let final_result = initial_result
        .and_then(add_one)
        .and_then(add_one)

    # Expected Output: The number 7
    printline("Final value: {{ final_result.success }}")

    # Expected Output: null
    printline("Error value: {{ final_result.error }}")
endfn
